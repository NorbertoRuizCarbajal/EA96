public static List<Integer> reverseArray(List<Integer> a) {
    // Write your code here

        List<Integer> listaInversa = new ArrayList<>();

        for (int i = a.size() - 1; i >= 0; i--) {
            listaInversa.add(a.get(i));
        }

        return listaInversa;

    }


